{
  "name": "angular-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "set-env": "node set-env.js",
    "prestart": "npm run set-env",
    "start": "ng serve",
    "prebuild": "npm run set-env",
    "build": "ng build --configuration production",
    "prewatch": "npm run set-env",
    "watch": "ng build --watch --configuration development",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist/angular-website/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.1",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  }
}
